<category><pattern>NTHWORD <set>number</set> *</pattern>
<template><think>
<set var="nth"><star/></set>
<set var="count">1</set>
<set var="words"><star index="2"/></set>
</think>
<condition>
<li><var>words</var><value>NIL</value> "<star index="2"/>" has only <map><name>predecessor</name><get var="count"/></map> words.</li>
<li><var>count</var><value><get var="nth"/></value> 
<first><get var="words"/></first></li>
<li><think>
<set var="words"><rest><get var="words"/></rest></set>
<set var="count"><map><name>successor</name><get var="count"/></map></set>
</think> <loop/></li>
</condition></template>
