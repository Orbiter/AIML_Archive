<category> 
<pattern>TTTGOODCHOICE *</pattern>
<template>
<think><set name="inp"><star/></set>
<condition name="inp">
<li value="1"><set name="s1">X</set><set name="cs1">X</set></li>
<li value="2"><set name="s2">X</set><set name="cs2">X</set></li>
