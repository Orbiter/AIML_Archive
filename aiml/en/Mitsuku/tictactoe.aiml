<!-- The computer chooses a random corner to play -->

<category> 
<pattern>TTTGETRANDOMCORNER</pattern>
<template>
<random>
<li>1</li>
<li>3</li>
<li>7</li>
<li>9</li>
</random>
</template>
</category>


