<!-- Spin the reels -->

<category>
<pattern>XSPIN1</pattern>
<template>
<think>
	<random>
		<li><set name="reel1">A</set></li>
		<li><set name="reel1">7</set></li>
		<li><set name="reel1">K</set></li>
		<li><set name="reel1">7</set></li>
		<li><set name="reel1">Q</set></li>
		<li><set name="reel1">7</set></li>
		<li><set name="reel1">J</set></li>
		<li><set name="reel1">K</set></li>
		<li><set name="reel1">Q</set></li>
		<li><set name="reel1">J</set></li>
	</random>
</think>
</template>
</category>

