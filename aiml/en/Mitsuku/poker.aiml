	<set name="suit9"><srai>PKDRAWSUIT</srai></set>
	<set name="suit10"><srai>PKDRAWSUIT</srai></set>
	<srai>PKADDKOUNT</srai>
	<condition name="kount">
		<li value="2"><srai>PKCHOOSEOWNCARDS</srai></li>
	</condition>
	<srai>PKCHECKDIFFCARDS</srai>
</think>
</template>
</category>


