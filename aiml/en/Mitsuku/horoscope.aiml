<category>
<pattern>_ MY HOROSCOPE *</pattern>
<template>
<srai>HOROSCOPE</srai>
</template>
</category>

<category>
<pattern>HOROSCOPE</pattern>
<template>
<think>
<set name="topic">Horoscope</set>
</think>
<condition>
<li name="sign" value="*">
<random>
<li><srai><get name="sign"/></srai></li>
</random>
</li>
<li>What star sign are you?</li>
</condition>  
</template>
</category>

<topic name="Horoscope">
<category>
<pattern>*</pattern>
<that>WHAT STAR SIGN ARE YOU</that>
